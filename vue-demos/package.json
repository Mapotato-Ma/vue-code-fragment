{
  "name": "vue-demos",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-upload": "run-s build cls zip cls upload cls unzip",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "upload": "scp ./mapotato.zip 腾讯云:/home/nginx/html/",
    "zip": "powershell -Command \"Compress-Archive -Path ./mapotato/* mapotato.zip -Force\"",
    "unzip": "ssh 腾讯云 unzip -o /home/nginx/html/mapotato.zip -d /home/nginx/html/mapotato",
    "cls": "cls"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "highlight.js": "^11.10.0",
    "monaco-editor": "^0.50.0",
    "monaco-themes": "^0.4.4",
    "rxjs": "^7.8.1",
    "vite-plugin-ejs": "^1.7.0",
    "vue": "^3.4.21",
    "vue-draggable-plus": "^0.3.5",
    "vue-router": "^4.3.0",
    "vue-showdown": "^4.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.14.11",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "sass": "^1.72.0",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-bundle-analyzer": "^0.10.3",
    "vite-plugin-markdown": "^2.2.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vue-tsc": "^2.0.6"
  }
}